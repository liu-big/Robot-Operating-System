<?xml version="1.0"?>
<!--
  package.xml

  Copyright (c) 2023. All rights reserved.

  test_tf2 包的 ROS 包定义文件。
  此文件定义了包的元数据、依赖项和构建信息。
-->
<package>
  <!-- 包的名称，必须是唯一的 -->
  <name>test_tf2</name>
  <!-- 包的版本号 -->
  <version>0.6.5</version>
  <!-- 包的简短描述 -->
  <description>
    tf2 单元测试
  </description>
  <!-- 包的作者 -->
  <author>Tully Foote</author>
  <author>Eitan Marder-Eppstein</author>
  <!-- 包的维护者及其电子邮件 -->
  <maintainer email="tfoote@osrfoundation.org">Tully Foote</maintainer>
  <!-- 包的许可证类型 -->
  <license>BSD</license>

  <!-- 包的官方网站或 wiki 页面 -->
  <url type="website">http://www.ros.org/wiki/geometry_experimental</url>
    
  <!-- 构建工具依赖，指定 catkin 的最低版本 -->
  <buildtool_depend version_gte="0.5.68">catkin</buildtool_depend>

  <!-- 构建时依赖项，这些包在编译此包时是必需的 -->
  <build_depend>rosconsole</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>rostest</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>tf2</build_depend>
  <build_depend>tf2_bullet</build_depend>
  <build_depend>tf2_ros</build_depend>
  <build_depend>tf2_geometry_msgs</build_depend>
  <build_depend>tf2_kdl</build_depend>
  <build_depend>tf2_msgs</build_depend>

  <!-- 运行时依赖项，这些包在运行此包时是必需的 -->
  <run_depend>rosconsole</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>rostest</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>tf2</run_depend>
  <run_depend>tf2_bullet</run_depend>
  <run_depend>tf2_ros</run_depend>
  <run_depend>tf2_geometry_msgs</run_depend>
  <run_depend>tf2_kdl</run_depend>
  <run_depend>tf2_msgs</run_depend>

  <!-- 测试依赖项，这些包在运行测试时是必需的 -->
  <test_depend>rosunit</test_depend>
  <test_depend>rosbash</test_depend>

</package>


